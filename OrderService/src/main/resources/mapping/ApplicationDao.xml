<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cctv.dao.ApplicationUserDao">

	<select id="queryList" resultType="ApplicationUserEntity">
		SELECT user_id as userId,user_name as userName,user_dir as userDir FROM `tab_application_user` 
	</select>
	<select id="queryObject" resultType="ApplicationUserEntity">
		select * from tab_application_user where user_id = #{value}
	</select>

</mapper>